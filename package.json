{
  "name": "ml_trader",
  "version": "1.1.0",
  "main": "src/index.js",
  "scripts": {
    "docker:build": "docker build --tag docker.pkg.github.com/vraid-systems/ml_trader/main:`cat IMAGE_VERSION` .",
    "docker:publish": "docker push docker.pkg.github.com/vraid-systems/ml_trader/main:`cat IMAGE_VERSION`",
    "docker:run": "docker run -d -p 3049:3049 --env WATCHLIST_CSV=Crypto:BTC,Stock:QQQ --volume ml_trader:/tmp/ml_trader docker.pkg.github.com/vraid-systems/ml_trader/main:`cat IMAGE_VERSION`",
    "pm2:start": "npx pm2 start src/index.js",
    "pm2:stop": "npx pm2 del 0",
    "lint": "eslint src",
    "start": "node src/index.js",
    "test": "jest src/"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^2.0.1",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.1.0",
    "pm2": "^4.4.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
